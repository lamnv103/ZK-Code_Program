{
  "name": "zk-transfer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "circom:compile": "circom circuits/balance_check.circom --r1cs --wasm --sym -o build/",
    "circom:setup": "snarkjs powersoftau new bn128 12 pot12_0000.ptau && snarkjs powersoftau contribute pot12_0000.ptau pot12_0001.ptau --name='First contribution' -v && snarkjs powersoftau prepare phase2 pot12_0001.ptau pot12_final.ptau -v",
    "circom:generate-keys": "snarkjs groth16 setup build/balance_check.r1cs pot12_final.ptau balance_check_0000.zkey && snarkjs zkey contribute balance_check_0000.zkey balance_check_0001.zkey --name='1st Contributor Name' -v && snarkjs zkey export verificationkey balance_check_0001.zkey verification_key.json"
  },
  "dependencies": {

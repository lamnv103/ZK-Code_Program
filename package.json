    "circomlib": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "crypto": "^1.0.1",
    "ffjavascript": "^0.2.60",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "snarkjs": "^0.7.3",
    "tailwind-merge": "^2.1.0",
